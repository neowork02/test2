{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local": "vue-cli-service serve --mode local",
    "dev": "vue-cli-service serve --mode dev",
    "stg": "vue-cli-service serve --mode stg",
    "dev-build": "vue-cli-service build --mode dev",
    "stg-build": "vue-cli-service build --mode stg",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.5.3",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.6.5",
    "es6-promise": "^4.2.8",
    "from-cdn": "^1.0.1",
    "html2canvas": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^2.3.1",
    "leaflet": "^1.7.1",
    "moment": "^2.29.1",
    "realgrid": "^2.3.4",
    "v-mask": "^2.2.4",
    "vue": "^2.6.14",
    "vue-2-breadcrumbs": "^0.8.0",
    "vue-awesome-notifications": "^3.1.1",
    "vue-i18n": "^8.24.5",
    "vue-moment": "^4.1.0",
    "vue-pdf": "^4.3.0",
    "vue-plugin-load-script": "^1.3.2",
    "vue-router": "^3.2.0",
    "vue2-daterange-picker": "^0.6.5",
    "vue2-google-maps": "^0.10.7",
    "vue2-leaflet": "^2.7.1",
    "vue2-simplert-plugin": "^0.6.0-beta.2",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "prettier": "2.3.2",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/standard",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-undef": "off",
      "indent": "off",
      "prefer-const": "off",
      "spaced-comment": "off",
      "no-unused-vars": "off",
      "no-new": "off",
      "camelcase": "off",
      "vue/no-unused-components": "off",
      "vue/max-attributes-per-line": "off",
      "vue/singleline-html-element-content-newline": "off",
      "vue/attributes-order": "off",
      "vue/html-self-closing": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
